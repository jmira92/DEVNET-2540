<vpn-service xmlns="urn:ietf:params:xml:ns:yang:ietf-l3vpn-ntw">  
	<vpn-id>vpn-101</vpn-id>  
	<ie-profiles>    
		<ie-profile>      
			<ie-profile-id>vpn-101-profile</ie-profile-id>      
			<rd>0:65000:101</rd>      
			<vpn-targets>        
				<vpn-target>          
					<id>1</id>          
					<route-targets>            
						<route-target>0:65000:101</route-target>          
					</route-targets>          
					<route-targets>            
						<route-target>0:65000:101</route-target>          
					</route-targets>          
					<route-target-type>both</route-target-type>        
				</vpn-target>        
				<vpn-policies>          
					<export-policy>VPN-101-ROUTE-POLICY</export-policy>        
				</vpn-policies>      
			</vpn-targets>    
		</ie-profile>  
	</ie-profiles>  
	<vpn-nodes>    
		<vpn-node>      
			<ne-id>Node-5</ne-id>      
			<local-autonomous-system>65000</local-autonomous-system>      
			<vpn-network-accesses>        
				<vpn-network-access>          
					<id>Node-5-vpn1</id>          
					<port-id>GigabitEthernet0/0/0/1</port-id>          
					<connection>            
						<encapsulation-type>tagged-int</encapsulation-type>            
						<tagged-interface>              
							<type>dot1q</type>              
							<dot1q-vlan-tagged>                
								<cvlan-id>101</cvlan-id>              
							</dot1q-vlan-tagged>            
						</tagged-interface>          
					</connection>          
					<ip-connection>            
						<ipv4>              
							<address-allocation-type xmlns:l3vpn-svc="urn:ietf:params:xml:ns:yang:ietf-l3vpn-svc">l3vpn-svc:static-address</address-allocation-type>              
							<static-addresses>                
								<primary-address>Node-5-vpn1-ipv4-address</primary-address>                
								<address>                  
									<address-id>Node-5-vpn1-ipv4-address</address-id>                  
									<provider-address>30.1.1.1</provider-address>                  
									<prefix-length>24</prefix-length>                
								</address>              
							</static-addresses>            
						</ipv4>          
					</ip-connection>          
					<routing-protocols>            
						<routing-protocol>              
							<id>Node-5-vpn1-routing</id>              
							<type>bgp</type>              
							<bgp>                
								<address-family>ipv4</address-family>                
								<neighbor>30.1.1.2</neighbor>                
								<peer-autonomous-system>65003</peer-autonomous-system>                
								<redistribute-connected-ipv4-af></redistribute-connected-ipv4-af>              
							</bgp>            
						</routing-protocol>          
					</routing-protocols>        
				</vpn-network-access>      
			</vpn-network-accesses>      
			<node-ie-profile>vpn-101-profile</node-ie-profile>    
		</vpn-node>    
		<vpn-node>      
			<ne-id>Node-4</ne-id>      
			<local-autonomous-system>65000</local-autonomous-system>      
			<vpn-network-accesses>        
				<vpn-network-access>          
					<id>Node-4-vpn1</id>          
					<port-id>GigabitEthernet0/0/0/1</port-id>          
					<connection>            
						<encapsulation-type>tagged-int</encapsulation-type>            
						<tagged-interface>              
							<type>dot1q</type>              
							<dot1q-vlan-tagged>                
								<cvlan-id>101</cvlan-id>              
							</dot1q-vlan-tagged>            
						</tagged-interface>          
					</connection>          
					<ip-connection>            
						<ipv4>              
							<address-allocation-type xmlns:l3vpn-svc="urn:ietf:params:xml:ns:yang:ietf-l3vpn-svc">l3vpn-svc:static-address</address-allocation-type>              
							<static-addresses>                
								<primary-address>Node-4-vpn1-ipv4-address</primary-address>                
								<address>                  
									<address-id>Node-4-vpn1-ipv4-address</address-id>                  
									<provider-address>30.1.2.1</provider-address>                  
									<prefix-length>24</prefix-length>                
								</address>              
							</static-addresses>            
						</ipv4>          
					</ip-connection>          
					<routing-protocols>            
						<routing-protocol>              
							<id>Node-4-routing</id>              
							<type>bgp</type>              
							<bgp>                
								<address-family>ipv4</address-family>                
								<neighbor>30.1.2.2</neighbor>                
								<peer-autonomous-system>65003</peer-autonomous-system>                
								<redistribute-connected-ipv4-af></redistribute-connected-ipv4-af>              
							</bgp>            
						</routing-protocol>          
					</routing-protocols>        
				</vpn-network-access>      
			</vpn-network-accesses>      
			<node-ie-profile>vpn-101-profile</node-ie-profile>    
		</vpn-node>  
	</vpn-nodes>
</vpn-service>